# LogCollector Service Environment
NODE_ENV=development
PORT=3001

# MongoDB Configuration
MONGODB_URI=mongodb://admin:safeops123@localhost:27017/safeops?authSource=admin

# RabbitMQ Configuration
RABBITMQ_URL=amqp://safeops:safeops123@localhost:5672
RABBITMQ_QUEUE=raw_logs

# Webhook Security
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
WEBHOOK_SECRET=your-webhook-secret-change-in-production

# Rate Limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX_REQUESTS=100

# Logging
LOG_LEVEL=debug
